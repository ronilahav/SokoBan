<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.VBox?>

<BorderPane prefHeight="153.0" prefWidth="215.0" xmlns="http://javafx.com/javafx/8.0.111" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.KeySettingWindowController">
   <left>
      <VBox prefHeight="200.0" prefWidth="98.0" spacing="5.0" BorderPane.alignment="CENTER">
         <children>
            <Button fx:id="upBtn" mnemonicParsing="false" onAction="#onUpClick" prefHeight="25.0" prefWidth="73.0" text="set UP" />
            <Button fx:id="downBtn" mnemonicParsing="false" onAction="#onDownClick" prefHeight="25.0" prefWidth="73.0" text="set DOWN" />
            <Button fx:id="leftBtn" mnemonicParsing="false" onAction="#onLeftClick" prefHeight="25.0" prefWidth="73.0" text="set LEFT" />
            <Button fx:id="rightBtn" mnemonicParsing="false" onAction="#onRightClick" prefWidth="73.0" text="set RIGHT" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
         <BorderPane.margin>
            <Insets />
         </BorderPane.margin>
      </VBox>
   </left>
   <center>
      <VBox prefHeight="200.0" prefWidth="82.0" spacing="15.0" BorderPane.alignment="CENTER">
         <children>
            <Label fx:id="upLbl" text="Label" />
            <Label fx:id="downLbl" text="Label" />
            <Label fx:id="leftLbl" text="Label" />
            <Label fx:id="rightLbl" text="Label" />
         </children>
         <padding>
            <Insets bottom="10.0" left="10.0" right="10.0" top="10.0" />
         </padding>
      </VBox>
   </center>
</BorderPane>
